all:	domain.o domainTest.o entity.o entityTest.o
	+$(MAKE) -C ../../

domain.o:
	g++ -std=c++17 -Wall -c ../../../domain.cpp

domainTest.o:
	g++ -std=c++17 -Wall -c ../../../domainTest.cpp

entity.o:
	g++ -std=c++17 -Wall -c ../../../entity.cpp

entityTest.o:
	g++ -std=c++17 -Wall -c ../../../entityTest.cpp
